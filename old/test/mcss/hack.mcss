// unicode escape
body{
    // simple value hack ;
    // if the hex value < 10 then no convert to unicode char
    left: 20px\9;
    // underscore hack
    _left: 30px;
    // start hack
    *left: 20px;
    // base escape value, convert to unicode
    left: 20px \cc11;
    filter: u("progid:DXImageTransform.Microsoft.gradient(startColorstr='%s', endColorstr='%s', GradientType=0)" % #fff, #ccc);
    filter: alpha(opacity= 1 / 5, stop = 2);
    filter: js('Math.pow(%d, %d)' % (4 6));
}
